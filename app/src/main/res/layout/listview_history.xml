<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="7dp"
    android:background="@drawable/list_selecor">


    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/history.list.view.name"
            android:id="@+id/textView3"
            android:layout_marginLeft="19dp"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="nombre"
            android:id="@+id/txtListName"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:textStyle="normal"
            android:textSize="15sp" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/history.list.view.autoplate"
            android:id="@+id/textView5"
            android:layout_marginLeft="36dp"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="placa"
            android:id="@+id/txtListAutoPlate"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:textStyle="normal"
            android:textSize="15sp" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/history.list.view.date"
            android:id="@+id/textView4"
            android:layout_marginLeft="33dp"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="fecha de la carrera"
            android:id="@+id/txtListDate"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:textStyle="normal"
            android:textSize="15sp" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/history.list.view.phone"
            android:id="@+id/textView7"
            android:layout_marginLeft="14dp"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="telefono"
            android:id="@+id/txtListAddress"
            android:layout_marginLeft="14dp"
            android:layout_marginRight="14dp"
            android:textStyle="normal"
            android:textSize="15sp" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/history.list.view.address"
            android:id="@+id/textView9"
            android:layout_marginLeft="10dp"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="direccion"
            android:id="@+id/txtListPhone"
            android:layout_marginLeft="14dp"
            android:layout_marginRight="14dp"
            android:textStyle="normal"
            android:textSize="15sp" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/history.list.view.company"
            android:id="@+id/textView11"
            android:layout_marginLeft="6dp"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="nombre de la compaÃ±ia"
            android:id="@+id/txtListCompany"
            android:layout_marginLeft="14dp"
            android:layout_marginRight="14dp"
            android:textStyle="normal"
            android:textSize="15sp" />
    </LinearLayout>

</LinearLayout>
